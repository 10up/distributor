{
  "name": "10up/distributor",
  "type": "wordpress-plugin",
  "authors": [
  	{
  		"name": "Taylor Lovett",
  		"email": "taylor.lovett@10up.com"
  	}
  ],
  "require": {
  	"php": ">=5.6"
  },
  "autoload": {
  	"psr-4": {
  		"Distributor\\": "includes/classes/"
  	}
  },
  "require-dev": {
    "10up/wp_mock": "dev-dev",
