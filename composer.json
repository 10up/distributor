{
  "name": "10up/distributor",
  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "Taylor Lovett",
      "email": "taylor.lovett@10up.com"
    }
  ],
  "repositories": [
    {
        "type":"package",
        "package": {
          "name": "georgestephanis/application-passwords",
          "version":"master",
          "source": {
              "url": "https://github.com/georgestephanis/application-passwords.git",
              "type": "git",
              "reference":"master"
            }
        }
    }
  ],
  "require": {
    "php": ">=5.6",
    "yahnis-elsts/plugin-update-checker": "^4.4",
    "georgestephanis/application-passwords": "master"
  },
  "autoload": {
    "psr-4": {
      "Distributor\\": "includes/classes/"
    }
  },

  "require-dev": {
    "10up/wpacceptance": "dev-master",
    "10up/wp_mock": "~0.4",
    "phpunit/phpunit": "~7.5",
    "10up/phpcs-composer": "dev-master"
  },
  "scripts": {
    "lint": "./vendor/bin/phpcs . --runtime-set testVersion 5.6-",
    "lint-fix": "phpcbf . --runtime-set testVersion 5.6-"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
